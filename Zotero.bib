	month = jan,
	year = {2008},
	keywords = {Endurance, Nutrition, Rehabilitation Medicine, Sports Medicine, carbohydrate, electrolytes, fluid replacement, glycogen, hydration},
	pages = {329--348}
}

@incollection{christopher_j._rasmussen_ms_nutritional_2008,
