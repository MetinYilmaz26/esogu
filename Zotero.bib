	urldate = {2012-07-04TZ},
	journal = {Wear},
	author = {Pieringer, A. and Kropp, W. and Thompson, D.J.},
	month = may,
	year = {2011},
	keywords = {Contact filter, Non-Hertzian contact, Time-domain modelling, Wheel/rail interaction},
	pages = {328--338}
}

@article{arslan_3-d_2012,
	title = {3-{D} {Rail}–{Wheel} contact analysis using {FEA}},
	volume = {45},
