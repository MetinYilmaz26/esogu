@article{almeida_evaluation_2011,
	title = {Evaluation of {Ni}–{Al} coatings processed by plasma transferred arc},
	volume = {27},
	issn = {0267-0844},
	url = {http://www.maneyonline.com/doi/abs/10.1179/026708410X12550773057866},
	doi = {10.1179/026708410X12550773057866},
	abstract = {Higher efficiency processes are strongly dependent on the ability to process materials/components that can sustain more aggressive service conditions. β-NiAl intermetallic alloys are known for exhibiting superior high temperature properties, but are difficult to process. This study evaluated Ni–Al coatings processed in situ by plasma transferred arc (PTA) welding. Coatings processed with mixtures of Ni and Al powders, Al content within the range 15–50 wt-\%, were analysed. In situ development of Ni aluminides was confirmed by X-ray diffraction analysis. Increasing aluminium (Al) content in the deposited mixture increased the penetration depth. The microstructure of coatings depended on the amount of Al in the deposited powder mixture associated with the exothermal synthesis of the β-NiAl intermetallic compound. The present study presents an important step towards the protection of components for equipment of higher efficiency processes, as results validated the potential of PTA hardfacing as a processing technique for surface tailoring.},
	number = {4},
	urldate = {2014-12-18TZ},
	journal = {Surface Engineering},
	author = {Almeida, V B and Takano, E H and Mazzaro, I and D’Oliveira, A S C M},
	month = may,
	year = {2011},
	pages = {266--271}
}

