@article{mendez_weldability_2004,
	title = {Weldability of austenitic manganese steel},
	volume = {153–154},
	issn = {0924-0136},
	shorttitle = {Proceedings of the {International} {Conference} in {Advances} in {Materials} and {Processing} {Technologies}},
