@article{fan_experimental_2009,
	title = {Experimental study of the effect of viscoelastic damping materials on noise and vibration reduction within railway vehicles},
	volume = {319},
	issn = {0022-460X},
	url = {http://www.sciencedirect.com/science/article/pii/S0022460X0800312X},
	doi = {10.1016/j.jsv.2008.03.071},
	abstract = {Interior noise and vibration reduction has become one important concern of railway operating environments due to the influence of increased speeds and reduced vehicle weights for energy efficiency. Three types of viscoelastic damping materials, bitumen-based damping material, water-based damping coating and butyl rubber damping material, were developed to reduce the vibration and noise within railway vehicles. Two sleeper carriages were furnished with the new materials in different patterns of constrained-layer and free-layer damping treatment. The measurements of vibration and noise were carried out in three running carriages. It is found that the reduction effect of damping treatments depends on the running speed. The unweighted root-mean-square acceleration is reduced by 0.08–0.79 and 0.06–0.49\&\#xa0;m/s2 for the carriage treated by bitumen-based as well as water-based damping materials and water-based damping material, respectively. The first two materials reduce vibration in a wider frequency range of 63–1000\&\#xa0;Hz than the last. It turns out that the damping treatments of the first two reduce the interior noise level by 5–8\&\#xa0;dBA within the carriage, and the last damping material by 1–6\&\#xa0;dBA. However, the specific loudness analysis of noises shows that the noise components between 125 and 250\&\#xa0;Hz are dominant for the overall loudness, although the low-frequency noise is noticeably decreased by the damping materials. The measure of loudness is shown to be more accurate to assess reduction effect of the damping material on the acoustic comfort.},
	number = {1–2},
	urldate = {2012-05-15TZ},
	journal = {Journal of Sound and Vibration},
	author = {Fan, Rongping and Meng, Guang and Yang, Jun and He, Caichun},
	year = {2009},
	pages = {58--76}
}

