@incollection{john_contact_2001,
	title = {Contact mechanics and lubrication hydrodynamics of chemical-mechanical planarization},
	volume = {Volume 39},
	isbn = {0167-8922},
	url = {http://www.sciencedirect.com/science/article/pii/S0167892201800936},
	abstract = {The technology of chemical-mechanical planarization (or polishing), CMP, has recently received considerable attention in the electronics manufacturing industry of the US. It is a cost-effective process to attain a high degree of required global planarity in the production of integrated circuit components. This paper reports on a fully three-dimensional finite element analysis of the stress and deformation field of a porous compliant polishing pad of finite thickness in contact with a rigid wafer; coupled to a FEM flow analysis of the slurry. The pad deformation is governed by the classical linear elasticity equations. The pad presents a rough porous surface to the wafer. The fluid flow between the pad and wafer is accounted for by the Reynolds equation. The linkage between the solid contact stress and the effective fluid film is through the Greenwood-Williamson model that relates the contact pressure of the pad asperities and the effective surface separation distance.

The pressure predictions of this model fit experimental results reasonably well. Surprisingly, for most conditions, the average fluid pressure at the silicon/pad interface is sub-ambient (suction). The overall fluid unit pressure can be approximately 50–100\% of the applied normal load. The contact normal stress is not symmetric about the wafer center due to the presence of contact friction. Greater contact stress causes a thinner film at the leading edge. Such a net diverging fluid film causes negative fluid pressures.},
	urldate = {2012-03-01TZ},
	booktitle = {Tribology {Research}: {From} {Model} {Experiment} to {Industrial} {Problem} {A} {Century} of {Efforts} in {Mechanics}, {Materials} {Science} and {Physico}-{Chemistry} {Proceedings} of the 27th {Leeds}-{Lyon} {Symposium} on {Tribology}},
	publisher = {Elsevier},
	author = {John, Tichy},
	collaborator = {G. Dalmaz, A.A. Lubrecht},
	year = {2001},
	pages = {63--68}
}

