@article{peter_high_2008,
	title = {High strength rail steels—{The} importance of material properties in contact mechanics problems},
	volume = {265},
	issn = {0043-1648},
	url = {http://www.sciencedirect.com/science/article/pii/S0043164808001841},
	doi = {10.1016/j.wear.2008.03.015},
	abstract = {The importance of material properties for solving rolling contact fatigue (RCF)—contact mechanics problems is shown. In addition to parameters like loading, geometry and friction, the contribution of the material to the system is very important. The determination and modelling of these material properties under the characteristic load conditions of a wheel–rail contact are unsolved problems in the scientific field of contact mechanics. A closer look at these material properties could contribute to a global solution.},
